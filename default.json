{
  "extends": [
    "config:base",
    ":automergeBranch",
    ":automergePatch",
    ":automergeLinters",
    ":automergeTesters",
    ":automergeTypes",
    ":maintainLockFilesWeekly",
    ":semanticCommitsDisabled"
  ],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchCurrentVersion": ">= 1.0.0",
      "updateTypes": [
        "minor"
      ],
      "automerge": true
    },
    {
      "matchDepTypes": ["devDependencies"],
      "updateTypes": [
        "minor"
      ],
      "automerge": true
    }
  ],
  "hostRules": [
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "npmrc": "@kaliber5:registry=https://npm.pkg.github.com/",
  "postUpdateOptions": ["yarnDedupeHighest"]
}